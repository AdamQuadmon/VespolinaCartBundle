{
    "name": "vespolina/cart-bundle",
    "type": "symfony-bundle",
    "description": "Vespolina Cart bundle. Cart management and more.",
    "keywords": ["shop", "ecommerce", "cart", "item"],
    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/VespolinaCartBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "dev-master",
        "symfony/framework-bundle": "2.*",
        "symfony/security-bundle": "2.*",
        "doctrine/doctrine-bundle": "master-dev",
        "doctrine/common": "master-dev",
        "doctrine/orm": "master-dev",
        "doctrine/dbal": "2.1.5",
        "doctrine/mongodb": "master-dev",
        "doctrine/mongodb-odm": "master-dev"
   },
    "suggest": {
        "symfony/doctrine-bundle": "*",
        "symfony/mongodb-odm-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Vespolina\\CartBundle": "" }
    },
    "target-dir": "Vespolina/CartBundle"
}

